library(testthat)
classify_penguin <- function(bill_length, flipper_length) {
if (flipper_length > 205){
return("Gentoo")
} else {
if (bill_length > 45) {
return("Chinstrap")
} else {
return("Adelle")
}
}
}
expect_equal(classify_penguin(40,200), "Gentoo"))
expect_equal(classify_penguin(40,210), "Gentoodsfj"))
expect_equal(classify_penguin(40,200), "Gentoo")
stopifnot(classify_penguin(40,210), "Gentoo")
